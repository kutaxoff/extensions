{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "lint:scss": "stylelint \"projects/**/*.scss\" --fix",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --base-href=/extensions/",
    "predeploy": "npm run build:docs",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "build:lib": "ng build extensions --configuration production",
    "build:moment-adapter": "ng build extensions-moment-adapter --configuration production",
    "build:luxon-adapter": "ng build extensions-luxon-adapter --configuration production",
    "build:date-fns-adapter": "ng build extensions-date-fns-adapter --configuration production",
    "publish": "npm run build:lib && cd dist/extensions && npm publish",
    "publish:moment-adapter": "npm run build:moment-adapter && cd dist/extensions-moment-adapter && npm publish",
    "publish:luxon-adapter": "npm run build:luxon-adapter && cd dist/extensions-luxon-adapter && npm publish",
    "publish:date-fns-adapter": "npm run build:date-fns-adapter && cd dist/extensions-date-fns-adapter && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.0",
    "@angular/localize": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/material-date-fns-adapter": "^14.2.0",
    "@angular/material-luxon-adapter": "^14.2.0",
    "@angular/material-moment-adapter": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@ng-select/ng-option-highlight": "0.0.7",
    "@ng-select/ng-select": "^9.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "date-fns": "^2.29.3",
    "luxon": "^3.0.4",
    "moment": "^2.29.4",
    "ngx-color": "^8.0.3",
    "ngx-progressbar": "^9.0.0",
    "photoviewer": "^3.6.5",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular-eslint/builder": "^14.0.3",
    "@angular-eslint/eslint-plugin": "^14.0.3",
    "@angular-eslint/eslint-plugin-template": "^14.0.3",
    "@angular-eslint/schematics": "^14.0.3",
    "@angular-eslint/template-parser": "^14.0.3",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/language-service": "^14.2.0",
    "@commitlint/cli": "^17.1.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.23.0",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.6.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.0.3",
    "markdown-loader": "^8.0.0",
    "ng-packagr": "^14.2.0",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "~10.8.0",
    "tsickle": "^0.46.0",
    "typescript": "~4.7.4"
  }
}
